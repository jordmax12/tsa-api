{
  "name": "tsa-api",
  "version": "1.0.0",
  "description": "TSA API",
  "main": "index.js",
  "license": "MIT",
  "engines": {
    "npm": "please-use-yarn",
    "yarn": ">=1.22",
    "node": "18"
  },
  "workspaces": {
    "packages": [
      "/*",
      "services/**",
      "packages/**"
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "test": "jest unit --detectOpenHandles",
